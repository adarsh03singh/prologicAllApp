<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.gstsuvidhakendra.mygsk">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <!--<uses-permission android:name="android.permission.RECEIVE_SMS" />-->

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="org.gstsuvidhakendra.mygsk.ServiceDetails"></activity>
        <activity android:name="org.gstsuvidhakendra.mygsk.AllServices" />
        <activity
            android:name="org.gstsuvidhakendra.mygsk.GskListScroll"
            android:label="@string/title_activity_gsk_list_scroll"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="org.gstsuvidhakendra.mygsk.GskList" />
        <activity
            android:name="org.gstsuvidhakendra.mygsk.InnerMainScreen"
            android:label="@string/title_activity_inner_main_screan"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="org.gstsuvidhakendra.mygsk.OtpInput"
            android:theme="@style/noActionBar"/>
        <activity android:name="org.gstsuvidhakendra.mygsk.MobileInput"
            android:theme="@style/noActionBar"/>
        <activity android:name="org.gstsuvidhakendra.mygsk.NameInput"
            android:theme="@style/noActionBar"/>
        <activity android:name="org.gstsuvidhakendra.mygsk.PinCodeInput"
            android:theme="@style/noActionBar"/>
        <activity android:name="org.gstsuvidhakendra.mygsk.MainActivity"
            android:theme="@style/noActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

       <!-- <receiver
            android:name="org.gstsuvidhakendra.mygsk.MyReceiver"
            android:exported="true">
            <intent-filter>
                <action android:name="android.provider.Telephony.SMS_RECEIVED" />
            </intent-filter>
        </receiver>-->
    </application>

</manifest>