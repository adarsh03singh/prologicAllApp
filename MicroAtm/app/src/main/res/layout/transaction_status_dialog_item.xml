<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <androidx.appcompat.widget.LinearLayoutCompat
        android:id="@+id/transactionDetailParentLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/tvStatusHeading"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20sp"
            android:gravity="start"
            android:layout_gravity="center"
            android:text="Cash Withdrawl Status"
            android:textColor="@color/blue"
            android:textAllCaps="false"
            android:textSize="15sp" />


        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="15sp"
            android:layout_marginEnd="15sp"
            android:layout_marginTop="15sp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="start"
                android:layout_weight="1"
                android:text="Invoice Number"
                android:textColor="@color/black"
                android:textAllCaps="false"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/tvInvoiceNo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="end"
                android:layout_weight="0.7"
                android:text="null"
                android:textColor="@color/gray_dark"
                android:textAllCaps="false"
                android:textSize="12sp" />

        </androidx.appcompat.widget.LinearLayoutCompat>

      <View
          android:layout_width="match_parent"
          android:layout_height="0.8dp"
          android:layout_marginTop="5sp"
          android:layout_marginStart="15sp"
          android:layout_marginEnd="15sp"
          android:background="@color/gray_dark"/>

        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="15sp"
            android:layout_marginEnd="15sp"
            android:layout_marginTop="15sp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="start"
                android:layout_weight="1"
                android:text="Auth Id"
                android:textColor="@color/black"
                android:textAllCaps="false"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/tvAuthId"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="end"
                android:layout_weight="0.7"
                android:text="null"
                android:textColor="@color/gray_dark"
                android:textAllCaps="false"
                android:textSize="12sp" />

        </androidx.appcompat.widget.LinearLayoutCompat>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.8dp"
            android:layout_marginTop="5sp"
            android:layout_marginStart="15sp"
            android:layout_marginEnd="15sp"
            android:background="@color/gray_dark"/>

        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="15sp"
            android:layout_marginEnd="15sp"
            android:layout_marginTop="15sp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="start"
                android:layout_weight="1"
                android:text="Terminal Id"
                android:textColor="@color/black"
                android:textAllCaps="false"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/tvTerminalId"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="end"
                android:layout_weight="0.7"
                android:text="null"
                android:textColor="@color/gray_dark"
                android:textAllCaps="false"
                android:textSize="12sp" />

        </androidx.appcompat.widget.LinearLayoutCompat>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.8dp"
            android:layout_marginTop="5sp"
            android:layout_marginStart="15sp"
            android:layout_marginEnd="15sp"
            android:background="@color/gray_dark"/>

        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="15sp"
            android:layout_marginEnd="15sp"
            android:layout_marginTop="15sp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="start"
                android:layout_weight="1"
                android:text="MID"
                android:textColor="@color/black"
                android:textAllCaps="false"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/tvMid"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="end"
                android:layout_weight="0.7"
                android:text="null"
                android:textColor="@color/gray_dark"
                android:textAllCaps="false"
                android:textSize="12sp" />

        </androidx.appcompat.widget.LinearLayoutCompat>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.8dp"
            android:layout_marginTop="5sp"
            android:layout_marginStart="15sp"
            android:layout_marginEnd="15sp"
            android:background="@color/gray_dark"/>

        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="15sp"
            android:layout_marginEnd="15sp"
            android:layout_marginTop="15sp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="start"
                android:layout_weight="1"
                android:text="Ref Id"
                android:textColor="@color/black"
                android:textAllCaps="false"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/tvRefId"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="end"
                android:layout_weight="0.7"
                android:text="null"
                android:textColor="@color/gray_dark"
                android:textAllCaps="false"
                android:textSize="12sp" />

        </androidx.appcompat.widget.LinearLayoutCompat>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.8dp"
            android:layout_marginTop="5sp"
            android:layout_marginStart="15sp"
            android:layout_marginEnd="15sp"
            android:background="@color/gray_dark"/>

        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="15sp"
            android:layout_marginEnd="15sp"
            android:layout_marginTop="15sp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="start"
                android:layout_weight="1"
                android:text="RRN"
                android:textColor="@color/black"
                android:textAllCaps="false"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/tvRRn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="end"
                android:layout_weight="0.7"
                android:text="null"
                android:textColor="@color/gray_dark"
                android:textAllCaps="false"
                android:textSize="12sp" />

        </androidx.appcompat.widget.LinearLayoutCompat>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.8dp"
            android:layout_marginTop="5sp"
            android:layout_marginStart="15sp"
            android:layout_marginEnd="15sp"
            android:background="@color/gray_dark"/>

        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="15sp"
            android:layout_marginEnd="15sp"
            android:layout_marginTop="15sp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="start"
                android:layout_weight="1"
                android:text="Batch Number"
                android:textColor="@color/black"
                android:textAllCaps="false"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/tvbatchNo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="end"
                android:layout_weight="0.7"
                android:text="null"
                android:textColor="@color/gray_dark"
                android:textAllCaps="false"
                android:textSize="12sp" />

        </androidx.appcompat.widget.LinearLayoutCompat>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.8dp"
            android:layout_marginTop="5sp"
            android:layout_marginStart="15sp"
            android:layout_marginEnd="15sp"
            android:background="@color/gray_dark"/>

        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="15sp"
            android:layout_marginEnd="15sp"
            android:layout_marginTop="15sp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="start"
                android:layout_weight="1"
                android:text="Card Number"
                android:textColor="@color/black"
                android:textAllCaps="false"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/tvcardNo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="end"
                android:layout_weight="0.7"
                android:text="null"
                android:textColor="@color/gray_dark"
                android:textAllCaps="false"
                android:textSize="12sp" />

        </androidx.appcompat.widget.LinearLayoutCompat>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.8dp"
            android:layout_marginTop="5sp"
            android:layout_marginStart="15sp"
            android:layout_marginEnd="15sp"
            android:background="@color/gray_dark"/>

        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="15sp"
            android:layout_marginEnd="15sp"
            android:layout_marginTop="15sp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="start"
                android:layout_weight="1"
                android:text="Date"
                android:textColor="@color/black"
                android:textAllCaps="false"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/tvDate"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="end"
                android:layout_weight="0.7"
                android:text="null"
                android:textColor="@color/gray_dark"
                android:textAllCaps="false"
                android:textSize="12sp" />

        </androidx.appcompat.widget.LinearLayoutCompat>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.8dp"
            android:layout_marginTop="5sp"
            android:layout_marginStart="15sp"
            android:layout_marginEnd="15sp"
            android:background="@color/gray_dark"/>

        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="15sp"
            android:layout_marginEnd="15sp"
            android:layout_marginTop="15sp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="start"
                android:layout_weight="1"
                android:text="Txn Amount"
                android:textColor="@color/black"
                android:textAllCaps="false"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/tvTxnAmount"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="end"
                android:layout_weight="0.7"
                android:text="null"
                android:textColor="@color/gray_dark"
                android:textAllCaps="false"
                android:textSize="12sp" />

        </androidx.appcompat.widget.LinearLayoutCompat>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.8dp"
            android:layout_marginTop="5sp"
            android:layout_marginStart="15sp"
            android:layout_marginEnd="15sp"
            android:background="@color/gray_dark"/>

        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="15sp"
            android:layout_marginEnd="15sp"
            android:layout_marginTop="15sp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="start"
                android:layout_weight="1"
                android:text="Account Balance"
                android:textColor="@color/black"
                android:textAllCaps="false"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/tvAmountBalance"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="end"
                android:layout_weight="0.7"
                android:text="null"
                android:textColor="@color/gray_dark"
                android:textAllCaps="false"
                android:textSize="12sp" />

        </androidx.appcompat.widget.LinearLayoutCompat>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.8dp"
            android:layout_marginTop="5sp"
            android:layout_marginStart="15sp"
            android:layout_marginEnd="15sp"
            android:background="@color/gray_dark"/>


        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="15sp"
            android:layout_marginEnd="15sp"
            android:layout_marginTop="25sp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="start"
                android:text="Remarks :"
                android:textColor="@color/black"
                android:textAllCaps="false"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/tvRemarks"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="end"
                android:text="Successfull"
                android:layout_marginStart="5sp"
                android:textColor="@color/gray_dark"
                android:textAllCaps="false"
                android:textSize="12sp" />

        </androidx.appcompat.widget.LinearLayoutCompat>

        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="15sp"
            android:layout_marginEnd="15sp"
            android:layout_marginTop="25sp"
            android:orientation="horizontal">

            <Button
                android:id="@+id/shareButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="30sp"
                android:layout_gravity="end"
                android:text="Share"/>

            <Button
                android:id="@+id/printButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="30sp"
                android:layout_marginStart="15sp"
                android:layout_marginEnd="15sp"
                android:layout_gravity="end"
                android:text="Print"/>
        </androidx.appcompat.widget.LinearLayoutCompat>




    </androidx.appcompat.widget.LinearLayoutCompat>

</androidx.constraintlayout.widget.ConstraintLayout>
