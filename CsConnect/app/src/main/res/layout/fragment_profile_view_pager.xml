<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:id="@+id/constraintLayout">


    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:id="@+id/cardView">

        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20sp"
            android:weightSum="3"
            android:orientation="vertical">

            <androidx.appcompat.widget.LinearLayoutCompat
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:weightSum="3"
                android:orientation="horizontal">

                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="100dp"
                    android:layout_height="120dp"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:layout_marginEnd="8dp"
                    android:layout_marginTop="65dp"
                    android:layout_marginStart="8dp">

                    <FrameLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginEnd="8dp"
                        android:layout_marginStart="8dp">

                        <de.hdodenhof.circleimageview.CircleImageView
                            android:id="@+id/broker_second"
                            android:layout_width="70dp"
                            android:layout_height="70dp"
                            android:layout_gravity="bottom|center_horizontal"
                            android:src="@drawable/richard_pic"
                            app:civ_border_color="#FF000000"/>

                    </FrameLayout>

                    <TextView
                        android:id="@+id/broker_second_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Richard"
                        android:textColor="@color/colorPrimary"
                        android:textSize="13sp"
                        android:layout_gravity="center"
                        tools:layout_editor_absoluteY="242dp"
                        tools:layout_editor_absoluteX="175dp"/>

                    <androidx.appcompat.widget.AppCompatRatingBar
                        style="@style/Widget.AppCompat.RatingBar.Small"
                        android:id="@+id/ratingBar2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:scaleX="0.7"
                        android:scaleY="0.7"
                        android:numStars="5"
                        android:rating="5"
                        android:max="5"
                        android:layout_gravity="center"
                        android:clickable="false"/>

                </androidx.appcompat.widget.LinearLayoutCompat>


                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="120dp"
                    android:layout_height="150dp"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    android:layout_gravity="center_horizontal"
                    android:id="@+id/linearLayout2">

                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center">

                        <de.hdodenhof.circleimageview.CircleImageView
                            android:id="@+id/broker_first"
                            android:layout_width="95dp"
                            android:layout_height="95dp"
                            android:layout_gravity="bottom|center_horizontal"
                            android:src="@drawable/david_john"
                            app:civ_border_color="#FF000000"/>

                        <de.hdodenhof.circleimageview.CircleImageView
                            android:id="@+id/camera_image"
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:layout_gravity="center_vertical|left"
                            android:src="@drawable/ic_top_performer"
                            app:civ_border_color="#FF000000"/>

                    </FrameLayout>

                    <TextView
                        android:id="@+id/broker_first_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="David John"
                        android:textSize="13sp"
                        android:textColor="@color/colorPrimary"
                        android:layout_gravity="center"
                        tools:layout_editor_absoluteY="242dp"
                        tools:layout_editor_absoluteX="175dp"/>

                    <androidx.appcompat.widget.AppCompatRatingBar
                        style="@style/Widget.AppCompat.RatingBar.Small"
                        android:id="@+id/ratingBar"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:isIndicator="true"
                        android:scaleX="0.7"
                        android:scaleY="0.7"
                        android:max="5"
                        android:numStars="5"
                        android:rating="5"
                        android:clickable="false"/>

                </androidx.appcompat.widget.LinearLayoutCompat>


                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="100dp"
                    android:layout_height="120dp"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    android:layout_marginStart="8sp"
                    android:layout_marginEnd="8dp"
                    android:id="@+id/linearLayout"
                    android:layout_marginTop="65dp">

                    <FrameLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginEnd="8dp"
                        android:layout_marginStart="8dp">

                        <de.hdodenhof.circleimageview.CircleImageView
                            android:id="@+id/broker_third"
                            android:layout_width="70dp"
                            android:layout_height="70dp"
                            android:layout_gravity="bottom|center_horizontal"
                            android:src="@drawable/gorge_pic"
                            app:civ_border_color="#FF000000"/>

                    </FrameLayout>

                    <TextView
                        android:id="@+id/broker_third_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Gorge"
                        android:textColor="@color/colorPrimary"
                        android:textSize="13sp"
                        android:layout_gravity="center"
                        tools:layout_editor_absoluteY="242dp"
                        tools:layout_editor_absoluteX="175dp"/>

                    <androidx.appcompat.widget.AppCompatRatingBar
                        style="@style/Widget.AppCompat.RatingBar.Small"
                        android:id="@+id/ratingBar4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:scaleX="0.7"
                        android:scaleY="0.7"
                        android:numStars="5"
                        android:max="5"
                        android:rating="5"
                        android:layout_gravity="center"/>

                </androidx.appcompat.widget.LinearLayoutCompat>
            </androidx.appcompat.widget.LinearLayoutCompat>

            <androidx.appcompat.widget.LinearLayoutCompat android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20sp"
                android:layout_marginBottom="5sp"
                android:orientation="vertical">

                <View android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#80BEBEBE"/>

                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginLeft="8sp"
                    android:layout_marginRight="8sp"
                    android:orientation="horizontal">


                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="5dp"
                        android:layout_marginLeft="5sp"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <de.hdodenhof.circleimageview.CircleImageView
                            android:id="@+id/broker_fourth"
                            android:layout_width="50dp"
                            android:layout_height="50dp"
                            android:layout_gravity="center_vertical"
                            android:src="@drawable/david_john"
                            app:civ_border_color="#FF000000"/>


                        <TextView
                            android:id="@+id/broker_forth_name"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Name"
                            android:textColor="@color/colorPrimary"
                            android:textSize="13sp"
                            android:paddingLeft="20sp"
                            android:layout_gravity="center"
                            tools:layout_editor_absoluteY="242dp"
                            tools:layout_editor_absoluteX="175dp"/>

                    </androidx.appcompat.widget.LinearLayoutCompat>


                    <androidx.appcompat.widget.AppCompatRatingBar
                        style="@style/Widget.AppCompat.RatingBar.Small"
                        android:id="@+id/ratingBar3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="10sp"
                        android:scaleX="0.7"
                        android:scaleY="0.7"
                        android:numStars="5"
                        android:max="5"
                        android:rating="5"
                        android:layout_gravity="center"/>


                </androidx.appcompat.widget.LinearLayoutCompat>

                <View android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#80BEBEBE"/>


                <View android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginTop="5sp"
                    android:background="#80BEBEBE"/>
                <androidx.appcompat.widget.LinearLayoutCompat
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="8sp"
                    android:layout_marginRight="8sp"
                    android:orientation="horizontal">


                    <androidx.appcompat.widget.LinearLayoutCompat
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="5dp"
                        android:layout_marginLeft="5sp"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <de.hdodenhof.circleimageview.CircleImageView
                            android:id="@+id/profile_fifth"
                            android:layout_width="50dp"
                            android:layout_height="50dp"
                            android:layout_gravity="center_vertical"
                            android:src="@drawable/win_diesel"
                            app:civ_border_color="#FF000000"/>


                        <TextView
                            android:id="@+id/broker_fifth_name"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Name"
                            android:textColor="@color/colorPrimary"
                            android:textSize="13sp"
                            android:paddingLeft="20sp"
                            android:layout_gravity="center"
                            tools:layout_editor_absoluteY="242dp"
                            tools:layout_editor_absoluteX="175dp"/>

                    </androidx.appcompat.widget.LinearLayoutCompat>


                    <androidx.appcompat.widget.AppCompatRatingBar
                        style="@style/Widget.AppCompat.RatingBar.Small"
                        android:id="@+id/ratingBar5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="10sp"
                        android:scaleX="0.7"
                        android:scaleY="0.7"
                        android:numStars="5"
                        android:max="5"
                        android:rating="5"
                        android:layout_gravity="center"/>

                </androidx.appcompat.widget.LinearLayoutCompat>
            </androidx.appcompat.widget.LinearLayoutCompat>

        </androidx.appcompat.widget.LinearLayoutCompat>
    </androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>